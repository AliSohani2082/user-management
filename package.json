{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "build": "next build",
    "clean": "rimraf .next out dist build",
    "dev": "next dev",
    "docker:build": "docker build -t user-management-spa .",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f",
    "docker:compose:up": "docker-compose up -d",
    "docker:run": "docker run -p 3000:3000 user-management-spa",
    "format": "prettier --write . && eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "eslint --ext .js,.jsx,.ts,.tsx . --max-warnings 0",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "start": "next start",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint:strict && npm run prettier && npm run test:ci"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run validate"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md,mdx}": [
      "prettier --write"
    ]
  },
